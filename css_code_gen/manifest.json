{
  "manifest_version": 3,
  "name": "CSS Class Finder",
  "version": "1.0",
  "description": "点击页面元素获取其class值",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "css": ["styles.css"]
  }],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}